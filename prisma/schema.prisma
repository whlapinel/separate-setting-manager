generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model students {
  firstName   String
  lastName    String
  id          String      @id
  testClass   String
  testClasses testClasses @relation(fields: [testClass], references: [id], onDelete: Restrict, onUpdate: Restrict)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model testClasses {
  id         String       @id(map: "testUnits_pkey")
  name       String
  block      Int
  occurrence String
  teacher    String
  students   students[]
  users      users        @relation(fields: [teacher], references: [id], onUpdate: Restrict)
  testEvents testEvents[]
}

model testEvents {
  testClass   String
  testDate    DateTime    @db.Date
  id          String      @id(map: "test_events_pkey")
  testName    String
  testClasses testClasses @relation(fields: [testClass], references: [id], onDelete: Restrict, onUpdate: Restrict)
}

model users {
  firstName   String
  lastName    String
  id          String        @id
  email       String
  role        String[]
  testClasses testClasses[]
}
